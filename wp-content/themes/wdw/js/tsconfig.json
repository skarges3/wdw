{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false
    },
    "filesGlob": [
        "**/*.ts",
        "**/*.tsx",
        "!typings/**",
        "!node_modules/**"
    ],
    "atom": {
        "rewriteTsconfig": true
    },
    "files": [
        "site.ts",
        "tabset.ts"
    ]
}
